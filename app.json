        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "ACCESS_COARSE_LOCATION",
        {
          "name": "ACCESS_FINE_LOCATION",
          "maxSdkVersion": 30
        }
      ]
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5660d4a0-3620-40c9-8675-34c4388d6015"
